{
	"tailwindCSS.experimental.classRegex": [
		[
			"ui:\\s*{([^)]*)\\s*}",
			"[\"'`]([^\"'`]*).*?[\"'`]"
		],
		[
			"'ui':\\s*{([^)]*)\\s*}",
			"[\"'`]([^\"'`]*).*?[\"'`]"
		],
		[
			"'ef-ui':\\s*{([^)]*)\\s*}",
			"[\"'`]([^\"'`]*).*?[\"'`]"
		],
		[
			"/\\*\\s?'ui'\\s?\\*/\\s*{([^;]*)}",
			":\\s*[\"'`]([^\"'`]*).*?[\"'`]"
		],
		[
			"/\\*\\s?('ui')\\s?\\*/\\s*{([^;]*)}",
			":\\s*[\"'`]([^\"'`]*).*?[\"'`]"
		],
		// https://github.com/paolotiu/tailwind-intellisense-regex-list?tab=readme-ov-file
		// add tv({...}) support
		[
			"tv\\(([^)]*)\\)",
			"{?\\s?[\\w].*:\\s*?[\"'`]([^\"'`]*).*?,?\\s?}?"
		],
	],
